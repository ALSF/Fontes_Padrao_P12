			aRotina := { {  STR0017,"AT470Visua",0,2 } }  //"Visualizar"
			AT470Visua("ABF", ABF->( Recno() ), 1 )
		Case cAlias == "ABB"
			Atc050Age( "AB6", AB6->( Recno() ), 1 )
		Case cAlias == "ADA"
