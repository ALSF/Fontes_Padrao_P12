		cQuery += "C6_TPCONTR='1' AND "
		cQuery += "D_E_L_E_T_<>'*' "
		cQuery += "ORDER BY C6_NUM, C6_ITEM"
		
		cQuery := ChangeQuery( cQuery )
		
