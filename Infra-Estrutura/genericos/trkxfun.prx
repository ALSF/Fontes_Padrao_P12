		cQuery += "SELECT C6_NUM, C6_ITEM FROM "+RetSqlName("SC6")+" SC6 "
		cQuery += "WHERE "
		cQuery += "C6_FILIAL='"  + xFilial( "SC6" ) + "' AND "
