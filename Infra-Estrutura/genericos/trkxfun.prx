					cQuery += "FROM " + RetSqlName( "SC7" ) + " "
					cQuery += "WHERE "
					cQuery += "C7_FILIAL='"  + xFilial( "SC7" ) + "' AND "
					cQuery += "C7_PLANILH='" + cPlanilha        + "' AND "
					cQuery += "C7_MEDICAO='" + cNumero          + "' AND "
					cQuery += "D_E_L_E_T_=' ' "
