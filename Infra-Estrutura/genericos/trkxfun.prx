			dbUseArea( .T., "TOPCONN", TcGenQry( , , cQuery ), cAliasQry, .F., .T. )
			
			oTree:TreeSeek( Pad( "SUA-" + cNumOrc, 50 )+cTreeID )
			
			While !( cAliasQry )->( Eof() )
				MATRKSUB( ( cAliasQry )->UB_NUM + ( cAliasQry )->UB_ITEM, @oTree, , @nLevel, nMaxLevel,,cTreeID)
				( cAliasQry )->( dbSkip() )
			End
			
