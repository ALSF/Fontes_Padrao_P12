					oTree:TreeSeek( Pad( "CND-" + cMedicao, 50 )+cTreeID )
					
					( cAliasQry )->( dbSkip() )
					
				End  
			Else			
				(cAliasQry) -> (dbGoTop())
				While !( cAliasQry )->( Eof() )
					
					If Empty( CND->CND_CLIENT )
						MATRKSC7H( ( cAliasQry )->C7_NUM, @oTree, , @nLevel, nMaxLevel, lItem,cTreeID )
					Else
						MATRKSC5( ( cAliasQry )->C5_NUM, @oTree, , @nLevel, nMaxLevel, lItem,cTreeID )
