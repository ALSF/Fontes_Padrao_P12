				MATRKSC5( ( cAliasQry )->UB_NUMPV, @oTree, , @nLevel, nMaxLevel, .F.,cTreeID )
				( cAliasQry )->( dbSkip() )
			End
			
			//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
