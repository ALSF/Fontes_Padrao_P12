		cAddString := If( ValType( cAddString ) == "C", cAddString, "" )
		cImageSUA  := MaEntImage( "SUA", 1 )
		
		oTree:AddItem( Pad( STR0044 + Transform( cNumOrc, "@r XXXXXX" ) + ; // "Atendimento televendas - "
