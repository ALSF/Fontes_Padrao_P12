			dbCloseArea()
			
			If Empty( CND->CND_CLIENT )
				dbSelectArea( "SC7" )
			Else
				dbSelectArea( "SC5" )
			EndIf
		
		EndIf 				

	EndIf

	nLevel--
EndIf

Return  


