If ( lContinua )

	DBSelectArea("SX5")
	DBSetOrder(1)

	If cTpNrNFS == "3"
		cQryAlias := GetNextAlias()

		cQuery := " SELECT X5_CHAVE SERIE, ( SELECT COALESCE(MAX(SD9.D9_DOC),' ') "
		cQuery += " FROM " + RetSqlName("SD9") +  " SD9 ""
	    
		If lGrpCNPJ
			cQuery += "	WHERE SD9.D9_CNPJ  = '" + SM0->M0_CGC + "'"
		Else
			cQuery += " WHERE SD9.D9_FILIAL = '" + xFilial("SD9") + "'"
		EndIF

		If lUsaNewKey
			cQuery += " AND SD9.D9_SDOC  = SX5.X5_CHAVE "
 		Else
			cQuery += " AND SD9.D9_SERIE = SX5.X5_CHAVE "
 		EndIf

		cQuery += " AND SD9.D9_DTUSO <> ' ' "
		cQuery += " AND SD9.D_E_L_E_T_ = ' ' ) MAX_DOCSD9, "

		If lGrpCNPJ
			aFilCGC := MatFilCgc(SM0->M0_CGC,"SF2") // Retorna o Compartilhamento conforme CNPJ
		EndIf

		cQuery += " ( SELECT COALESCE(MAX(SF2.F2_DOC),' ') "
		cQuery += " FROM " + RetSqlName("SF2") +  " SF2"
		
		If lGrpCNPJ
			cQuery += " WHERE SF2.F2_FILIAL IN " + FormatIN(aFilCGC[1],aFilCGC[2])
		Else
			cQuery += " WHERE SF2.F2_FILIAL = '" + xFilial("SD9") + "'"
		EndIF

		If lUsaNewKey
			cQuery += " AND SF2.F2_SDOC  = SX5.X5_CHAVE "
		Else
			cQuery += " AND SF2.F2_SERIE = SX5.X5_CHAVE "
		EndIf

		cQuery += " AND SF2.D_E_L_E_T_ = ' ' ) MAX_DOCSF2 "

		cQuery += " FROM " + RetSqlName("SX5") + " SX5 "
		cQuery += " WHERE SX5.X5_TABELA = '" + cTab + "'"
		cQuery += " AND SX5.X5_FILIAL = '" + cFilSX5 + "'"

		//Ignora as series do SIGALOJA
		cQuery += " AND SX5.X5_CHAVE NOT IN ( 'CPF', 'CP' )"
		cQuery +="  AND SX5.D_E_L_E_T_ = ' '

		cQuery := ChangeQuery(cQuery)

		DBUseArea(.T.,"TOPCONN",TcGenQry(,,cQuery),cQryAlias,.T.,.T.)

		While (cQryAlias)->(!Eof())

			If SX5->( DBSeek(cFilSX5+cTab+(cQryAlias)->SERIE) )

				If cPaisloc == "BRA" .And. Len(AllTrim(SX5->X5_DESCRI)) < Len(SF2->F2_DOC)
					nTamNota := 6
				Else
					nTamNota := Len(SF2->F2_DOC)
				EndIf

				If !Empty((cQryAlias)->MAX_DOCSD9)
					cNDocMax := Soma1( AllTrim( (cQryAlias)->MAX_DOCSD9 ), nTamNota )
				ElseIf !Empty((cQryAlias)->MAX_DOCSF2)
					cNDocMax := Soma1( AllTrim( (cQryAlias)->MAX_DOCSF2 ), nTamNota )
				Else
					cNDocMax := MaConvNNota( X5Descri(,,,,.F.), nTamNota )
				EndIf

