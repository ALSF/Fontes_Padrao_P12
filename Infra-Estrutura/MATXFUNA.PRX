	If M2_MOEDA&cMoeda != 0
		nMedia += M2_MOEDA&cMoeda
		nDias++
	EndIf

	DbSkip( )

End

DbSelectArea( cAlias )

Return ( Iif ( nMedia != 0 , nMedia / nDias, 0 ) )

