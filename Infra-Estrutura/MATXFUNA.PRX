If cLandPad$__cStrCWO
	nx	:=aScan(__aArrCWO,{|x| allTrim(x[1])==allTrim(cfilcwo) .AND. allTrim(x[2])==allTrim(cLandPad)})

	do while  nx<=len(__aArrCWO) .and. nx>0 .AND.   cfilcwo==__aArrCWO[nx,1] .AND. cLandPad ==__aArrCWO[nx,2]


