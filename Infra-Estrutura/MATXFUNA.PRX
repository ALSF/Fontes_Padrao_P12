FUNCTION RecMoeda(dData,cMoeda)

Local aArea   	:= GetArea()
Local nMoeda
Local nX, cX

DEFAULT __aMoeda := {CTOD("  /  /  ")}

If ValType(dData) == "C"
	dData := STOD(dData)
EndIf
If ( ValType(cMoeda) = "N" )
	cMoeda := AllTrim(Str(cMoeda,2))
EndIf

If ( Empty(cMoeda) .Or. cMoeda == "0" )
	cMoeda := "1"
EndIf
nMoeda := Val(cMoeda)

If Empty(dData)       // Nao Pediu Nada
	Return 0
EndIf

If nMoeda == 1
	Return 1
EndIf

If __aMoeda[1] == dData
	Return __aMoeda[nMoeda]
EndIf

DbSelectArea("SM2")
If Len(__aMoeda) == 1          // Ainda nao vi o Tamanho do Array
	For nX := 2 to 99
		cX := AllTrim(Str(nX,2,0))
		If FieldPos("M2_MOEDA"+cX) > 0
			AADD(__aMoeda,0)
		Else
			Exit
		EndIf
	Next
EndIf

//谀哪哪哪哪哪哪哪哪哪哪哪哪哪哪哪哪哪哪哪哪哪哪哪哪哪哪哪哪哪哪哪哪哪哪目
//Posiciona arquivo SM2 na data mais proxima                  ?
//滥哪哪哪哪哪哪哪哪哪哪哪哪哪哪哪哪哪哪哪哪哪哪哪哪哪哪哪哪哪哪哪哪哪哪馁
DbSeek(dData,.T.)
If ( Eof() )
	DbSkip(-1)
EndIf

__aMoeda[1] := dData
For nX := 2 to Len(__aMoeda)
	cX := AllTrim(Str(nX,2,0))
	__aMoeda[nX] := FieldGet(FieldPos("M2_MOEDA"+cX))
Next

RestArea(aArea)

Return( __aMoeda[nMoeda] )

