	cQuery := " SELECT COUNT(*) QTD_SF2" + CRLF
	cQuery += " FROM " +  RetSqlName("SF2") + " SF2" + CRLF
	cQuery += " WHERE" + CRLF
	cQuery += "    SF2.F2_FILIAL      " + IIf( lGrpCNPJ, "in " + FormatIN(aFilCGC[1],aFilCGC[2]), " = '" + xFilial("SF2") + "'" ) + CRLF
